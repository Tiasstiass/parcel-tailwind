{%- comment -%}
  Inputs:
    object - section or block with settings
{%- endcomment -%}

<div class="logo-bar-section" id="logo-bar--{{ section.id }}">
  <div class="page-width logo-bar__wrapper">
    {%- if section.settings.title != blank -%}
    <h3>{{ section.settings.title }}</h3>
    {%- endif -%}

    {%- for block in object.blocks -%}
      <div class="logo-bar__item-wrapper">
        {%- render 'image', image: block.settings.image -%}
      </div>
    {%- endfor -%}
  </div>
</div>



            

